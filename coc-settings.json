{
  "suggest.enablePreview": true,
  "coc.preferences.colorSupport": false,
  "java.signatureHelp.enabled": true,
  "python.jediEnabled": false,
  "signature.preferShownAbove": true,
  "signature.hideOnTextChange": false,
  "snippets.ultisnips.enable": true,
  "python.formatting.provider": "black",
  "rust.racer_completion": true,
  "rust.all_targets": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "vue-html": "html",
    "razor": "html"
  },
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.jumpCommand": "edit",
  "snippets.userSnippetsDirectory": "~/HyVim/ultisnips",
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": false,
  "git.virtualTextPrefix": "     ----- ",
  "list.normalMappings": {
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "d": "action:delete"
  },
  "list.insertMappings": {
    "<C-t>": "action:tabe",
    "<C-v>": "action:vsplit",
    "<C-x>": "action:split",
    "<C-w>": "command:wincmd k"
  },
  "coc.source.issues.enable": false,
  "list.maxHeight": 20,
  "snippets.extends": {
    "typescript.tsx": [
      "typescriptreact",
      "typescript",
      "javascript"
    ],
  },
  "yank.enableCompletion": false,
  "clangd.semanticHighlighting": true,
  "java.errors.incompleteClasspath.severity": "ignore"
}
