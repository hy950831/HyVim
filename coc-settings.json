{
  "suggest.enablePreview": true,
  "coc.preferences.colorSupport": false,
  "java.signatureHelp.enabled": true,
  "python.jediEnabled": false,
  "signature.preferShownAbove": true,
  "signature.hideOnTextChange": false,
  "snippets.ultisnips.enable": false,
  "python.formatting.provider": "black",
  "rust.racer_completion": true,
  "rust.all_targets": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "vue-html": "html",
    "razor": "html"
  },
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "git.addGlameToBufferVar": true,
  "git.addGlameToVirtualText": true,
  "coc.source.issues.enable": false,
  "list.maxHeight": 20,
  "git.virtualTextPrefix": "     ----- ",
  "snippets.extends": {
    "typescript.tsx": [
      "typescriptreact",
      "typescript",
      "javascript"
    ],
  },
  "languageserver": {
    "clangd": {
      "command": "clangd-9",
      "rootPatterns": [
        "compile_flags.txt",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ],
    }
  }
}
